{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "paths": {
            "@/*": ["./*"],
            "@src/*": ["./src/*"],
            "@app/*": ["./src/app/*"],
            "@assets/*": ["./src/assets/*"],
            "@components/*": ["./src/components/*"],
            "@constants/*": ["./src/constants/*"],
            "@context/*": ["./src/context/*"]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "src/components/HomeIcon.jsx",
        "src/app/_layout.jsx",
        "src/app/(app)/home/index.jsx",
        "src/app/(app)/_layout.jsx",
        "src/app/(app)/user/index.jsx",
        "src/app/(app)/enquiry/index.jsx",
        "src/constants/Colors.js",
        "src/components/CustomDropDown.jsx"
    ]
}
